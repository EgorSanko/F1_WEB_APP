# Telegram Bot Token (get from @BotFather)
TELEGRAM_TOKEN=your_bot_token_here

# WebApp URL (your domain with HTTPS)
WEBAPP_URL=https://yourdomain.com

# Admin Telegram User IDs (comma-separated)
ADMIN_IDS=123456789

# Database path (inside Docker)
DATABASE_PATH=/app/data/f1hub.db

# Debug mode (true/false)
DEBUG=false

# VK API Service Key (for video feeds, optional)
VK_SERVICE_KEY=
